{
	"frameworkName": "Nuxt",
	"firstCommand": "npm run dev",
	"projectTests": [
		{
			"arg": "-f",
			"path": "nuxt.config.js",
			"reason": "No `nuxt.config.js` file found"
		},
		{
			"arg": "-d",
			"path": "pages",
			"reason": "No `pages` folder found"
		}
	],
	"bootstraper": ["npx", ["create-nuxt-app"]],
	"actions": [
		{
			"zipPath": "pages/_uid.vue",
			"overwrite": false,
			"bootstrapped": true,
			"template": true
		},
		{
			"zipPath": "pages/index.vue",
			"overwrite": false,
			"bootstrapped": true,
			"template": true
		},
		{
			"zipPath": "custom_types/",
			"overwrite": false,
			"bootstrapped": true
		},
		{
			"type": "delete",
			"zipPath": "custom_types/"
		},
		{
			"type": "delete",
			"zipPath": "pages/"
		}
	],
	"dependencies": ["prismic-javascript", "prismic-vue", "vue-slicezone", "@nuxtjs/prismic"],
	"devDependencies": ["node-sass", "sass-loader"]
}
