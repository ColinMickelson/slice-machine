{
	"name": "@slicemachine/core2",
	"version": "0.1.0",
	"description": "package_description",
	"private": true,
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com/prismicio/slice-machine.git"
	},
	"license": "Apache-2.0",
	"author": "Prismic <contact@prismic.io> (https://prismic.io)",
	"type": "module",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"build": "vite build",
		"dev": "vite build --watch",
		"format": "prettier --write .",
		"lint": "eslint --ext .js,.ts .",
		"prepare": "npm run build",
		"release": "npm run test && standard-version && git push --follow-tags && npm run build && npm publish",
		"release:alpha": "npm run test && standard-version --release-as patch --prerelease alpha && git push --follow-tags && npm run build && npm publish --tag alpha",
		"release:alpha:dry": "standard-version --release-as patch --prerelease alpha --dry-run",
		"release:dry": "standard-version --dry-run",
		"size": "size-limit",
		"test": "npm run lint && npm run unit && npm run build && npm run size",
		"unit": "vitest run --coverage",
		"unit:watch": "vitest watch"
	},
	"dependencies": {
		"@prismicio/types": "^0.2.3",
		"@slicemachine/plugin-kit": "^0.1.2",
		"busboy": "^1.6.0",
		"devalue": "^4.2.0",
		"fp-ts": "^2.13.1",
		"h3": "^0.8.6",
		"io-ts": "^2.2.19",
		"io-ts-reporters": "^2.0.1",
		"jiti": "^1.16.0",
		"node-fetch": "^3.2.10",
		"puppeteer": "^19.2.0"
	},
	"devDependencies": {
		"@size-limit/preset-small-lib": "^8.1.0",
		"@types/busboy": "^1.5.0",
		"@types/formidable": "^2.0.5",
		"@typescript-eslint/eslint-plugin": "~5.41.0",
		"@typescript-eslint/parser": "^5.41.0",
		"@vitest/coverage-c8": "^0.24.3",
		"eslint": "^8.26.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-tsdoc": "^0.2.17",
		"prettier": "^2.7.1",
		"prettier-plugin-jsdoc": "^0.4.2",
		"siroc": "^0.16.0",
		"size-limit": "^8.1.0",
		"standard-version": "^9.5.0",
		"typescript": "^4.8.4",
		"vite": "^3.2.1",
		"vite-plugin-sdk": "^0.0.3",
		"vitest": "^0.24.3"
	},
	"engines": {
		"node": ">=14.15.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
